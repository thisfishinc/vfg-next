{
  "name": "vfg-next",
  "version": "3.00.4",
  "description": "A schema-based form generator component for Vue.js 3",
  "homepage": "https://github.com/bagus/vfg-next/",
  "private": false,
  "main": "dist/vfg-next.min.js",
  "module": "dist/vfg-next.esm.js",
  "jsnext:main": "dist/vfg-next.esm.js",
  "unpkg": "dist/vfg-next.umd.js",
  "jsdelivr": "dist/vfg-next.umd.js",
  "style": "dist/vfg-next.css",
  "files": [
    "src",
    "dist"
  ],
  "author": "Icebob",
  "license": "MIT",
  "scripts": {
    "build": "npx rimraf dist && rollup -c --environment BUILD:production",
    "dev": "npx rimraf dist && rollup -c -w --environment BUILD:production",
    "lint": "vue-cli-service lint"
  },
  "keywords": [
    "vue",
    "vue3",
    "vuejs",
    "form",
    "generator",
    "schema",
    "json"
  ],
  "devDependencies": {
    "@atomico/rollup-plugin-sizes": "^1.1.4",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@betit/rollup-plugin-rename-extensions": "^0.1.0",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-url": "^6.0.0",
    "@vue/cli-plugin-eslint": "^4.5.4",
    "@vue/cli-service": "^4.5.4",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "autoprefixer": "^10.2.4",
    "babel-eslint": "10.1.0",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.5.0",
    "lint-staged": "^10.2.11",
    "postcss": "^8.2.4",
    "postcss-import": "^14.0.0",
    "postcss-nested": "^5.0.3",
    "postcss-simple-vars": "^6.0.2",
    "postcss-url": "^10.1.1",
    "precss": "^4.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.3",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "sass": "^1.32.5",
    "vue": "^3.0.5"
  },
  "peerDependencies": {
    "fecha": "^4.2.0",
    "lodash": "^4.17.20"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint"
    ]
  }
}
